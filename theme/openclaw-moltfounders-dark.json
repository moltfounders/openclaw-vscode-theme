      "settings": {
        "background": "#5a1e02",
        "foreground": "#ffb366"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#121212",
        "background": "#5bc5f2"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#c792ea",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#5bc5f2"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#5bc5f2"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#5bc5f2"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
