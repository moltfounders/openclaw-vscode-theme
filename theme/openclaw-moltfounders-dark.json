      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#ffb366"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#5bc5f2"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#7ee081"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F5F5F7"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F5F5F7"
      }
    },
    {
      "scope": [
        "markup.underline"
      ],
      "settings": {
